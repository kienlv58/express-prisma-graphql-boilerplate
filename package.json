{
  "name": "express-ts-jest-prisma-graphql-boilerplate",
  "version": "1.0.4",
  "description": "ExpressJS App implemented with Typescript and Jest Testing Framework. Integrate prisma and graphql",
  "main": "./dist/app.js",
  "scripts": {
    "prebuild": "eslint --ext tslint.json",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "dev": "dotenv -e .env.development -- nodemon --watch '*.ts' --watch '*.json' --exec 'ts-node --files' ./src/server.ts",
    "setup": "env $(cat .env.production | grep -v \"#\" | xargs) pm2 deploy ecosystem.config.js production setup",
    "test": "dotenv -e .env.test -- jest --detectOpenHandles -c ./jest.config.js",
    "test:coverage": "dotenv -e .env.test -- jest --coverage -c ./jest.config.js",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts && eslint tests/**/*.ts",
    "lint-fix": "eslint --fix 'src/**/*.ts' && eslint --fix 'tests/**/*.ts'",
    "pretty": "prettier --write 'src/**/*.ts' && prettier --write 'tests/**/*.ts'",
    "postinstall": "npx husky install",
    "prepare": "husky install",
    "precommit": "npm run lint-fix && npm run pretty",
    "prepush": "npm run lint && npm run test:coverage",
    "seed": "prisma db seed",
    "migrate": "prisma migrate dev",
    "db-push": "prisma db push",
    "db-reset": "prisma migrate reset",
    "create-migrate": "prisma migrate dev --create-only",
    "migrate-deploy": "prisma migrate deploy",
    "generate-type": "prisma generate"
  },
  "keywords": [
    "express",
    "node",
    "nodejs",
    "jest",
    "typescript",
    "node-typescript",
    "backend",
    "backend-api",
    "husky",
    "eslint",
    "prettier",
    "starter-kit"
  ],
  "author": "Kienlv",
  "license": "Apache-2.0",
  "dependencies": {
    "@prisma/client": "4.10.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "envalid": "^7.3.1",
    "express": "^4.17.2",
    "express-actuator": "^1.8.0",
    "express-graphql": "^0.12.0",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-query-complexity": "^0.12.0",
    "graphql-scalars": "^1.20.1",
    "graphql-type-json": "^0.3.2",
    "helmet": "^5.0.2",
    "mongoose": "^6.2.1",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-actuator": "^1.8.0",
    "@types/graphql-fields": "^1.3.4",
    "@types/jest": "^27.4.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^17.0.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prisma": "4.10.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typegraphql-prisma": "^0.23.5",
    "typescript": "^4.5.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
